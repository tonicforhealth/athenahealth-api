{
    "name": "tonicforhealth/athenahealth-api",
    "description": "A simple wrapper for athenahealth API services",
    "homepage": "https://github.com/tonicforhealth/athenahealth-api",
    "license": "MIT",
    "keywords": ["tonicforhealth", "athenahealth"],
    "authors": [
        {
            "name": "Vitalii Ekert",
            "email": "vitalii.ekert@tonicforhealth.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^5.5|^7.0",
        "php-http/client-implementation": "^1.0",
        "psr/http-message-implementation": "^1.0",
        "php-http/client-common": "^1.0",
        "php-http/message": "^1.0",
        "php-http/plugins": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8",
        "phpunit/php-invoker": "^1.1",
        "php-http/guzzle6-adapter": "^1.0",
        "php-http/discovery": "^0.8",
        "puli/composer-plugin": "^1.0@beta"
    },
    "autoload": {
        "psr-4": {
            "TonicForHealth\\AthenaHealth\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TonicForHealth\\AthenaHealth\\Tests\\": "tests/"
        }
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
